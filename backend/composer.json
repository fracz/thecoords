{
    "name": "fracz/thecoords",
    "require": {
      "php": "^8.1",
      "beberlei/assert": "^3.3",
      "beberlei/doctrineextensions": "^1.3",
      "doctrine/annotations": "^1.13",
      "doctrine/migrations": "^3.5",
      "doctrine/orm": "^2.11",
      "gumlet/php-image-resize": "^2.0",
      "monolog/monolog": "^2.5",
      "myclabs/php-enum": "^1.8",
      "php-di/php-di": "^6.3",
      "ramsey/uuid-doctrine": "^1.8",
      "slim/psr7": "^1.5",
      "slim/slim": "^4.10",
      "symfony/cache": "^5.4",
      "symfony/console": "^5.4",
      "symfony/yaml": "^5.4",
      "tuupola/slim-jwt-auth": "^3.6"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5.20",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8082 -t public",
        "test": "phpunit"
    }
}
